{
  "StorageType": "local",
  "LocalStoragePath": "C:\\ConverterProjectStorage",
  "ConnectionStrings": {
    "BlobStorageAccount": "<yourAzureBlobStorageConnectionString>"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MaximumFilesPerUser": 10,
  "MaximumFileSize": 10485760,
  "TimeLimitAfterConversion": 15,
  "TimeLimitAfterDownload": 5
}
